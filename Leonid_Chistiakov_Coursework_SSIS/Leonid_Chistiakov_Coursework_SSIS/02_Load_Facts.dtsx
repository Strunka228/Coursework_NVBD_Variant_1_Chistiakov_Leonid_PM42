                <property
                  dataType="System.String"
                  description="Указывает переменную, содержащую имя объекта базы данных, используемого для открытия набора строк."
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="Команда SQL для выполнения."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT 
    bl.LoanID,
    bl.StudentID,
    bl.BookID,
    CAST(CONVERT(VARCHAR(8), bl.LoanDate, 112) AS INT) AS LoanDateKey,
    CAST(CONVERT(VARCHAR(8), bl.DueDate, 112) AS INT) AS DueDateKey,
    CASE 
        WHEN bl.ReturnDate IS NOT NULL THEN CAST(CONVERT(VARCHAR(8), bl.ReturnDate, 112) AS INT) 
        ELSE NULL 
    END AS ReturnDateKey,

    DATEDIFF(day, bl.LoanDate, ISNULL(bl.ReturnDate, GETDATE())) AS LoanDurationDays,

    CASE 
        WHEN bl.ReturnDate &gt; bl.DueDate THEN DATEDIFF(day, bl.DueDate, bl.ReturnDate)
        WHEN bl.ReturnDate IS NULL AND GETDATE() &gt; bl.DueDate THEN DATEDIFF(day, bl.DueDate, GETDATE())
        ELSE 0 
    END AS OverdueDays,

    CASE 
        WHEN (bl.ReturnDate &gt; bl.DueDate) OR (bl.ReturnDate IS NULL AND GETDATE() &gt; bl.DueDate) THEN 1 
        ELSE 0 
    END AS IsOverdue

FROM dbo.BookLoans bl</property>
